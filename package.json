{
  "type": "module",
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0"
  },
  "scripts": {
    "start": "npm-run-all -p dev:css dev:11ty",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -c tailwind.config.js -i tailwind.css -o _site/tailwind.css --watch --postcss",
    "build": "npm-run-all build:11ty build:css",
    "build:11ty": "eleventy",
    "build:css": "yarn tailwindcss -c tailwind.config.js -i tailwind.css -o _site/tailwind.css --postcss"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "sass": "^1.83.0",
    "tailwindcss": "^3.4.17"
  }
}
